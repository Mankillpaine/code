{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen","memcachedcloud", "memcached"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "ext-memcached": "*",
        "illuminate/support": "4.*",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "psr-0": {
            "kintso\\MemcachedCloud": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
